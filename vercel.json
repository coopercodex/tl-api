{
  "version": 2,
  "builds": [
      {
          "src": "index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/main.ts",
          "methods": [
              "GET",
              "POST",
              "PUT",
              "DELETE",
              "OPTIONS"
          ]
      }
  ],
 "headers": [
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "access-control-allow-origin",
              "value": "*"
            }
          ]
        }
      ]
    }
  